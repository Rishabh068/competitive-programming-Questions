#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
  string table;
  cin>>table;
  string ans="NO";
  for(int i=0;i<5;i++){
      string str;
      cin>>str;
      if(str[0]==table[0]||str[1]==table[1]){
          ans="YES";
          break;
      }
  }
  cout<<ans;
    return 0;
	
}
